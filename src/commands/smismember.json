{
    "SMISMEMBER": {
        "summary": "Returns the membership associated with the given elements for a set",
        "complexity": "O(N) where N is the number of elements being checked for membership",
        "group": "set",
        "since": "6.2.0",
        "arity": -3,
        "function": "smismemberCommand",
        "command_flags": [
            "READONLY",
            "FAST"
        ],
        "acl_categories": [
            "SET"
        ],
        "key_specs": [
            {
                "flags": [
                    "RO",
                    "ACCESS"
                ],
                "begin_search": {
                    "index": {
                        "pos": 1
                    }
                },
                "find_keys": {
                    "range": {
                        "lastkey": 0,
                        "step": 1,
                        "limit": 0
                    }
                }
            }
        ],
        "reply_schema": {
            "type": "array",
            "description": "List representing the membership of the given elements, in the same order as they are requested.",
            "minItems": 1,
            "items": {
                "oneOf": [
                    {
                        "const": 0,
                        "description": "Not a member of the set or the key does not exist."
                    },
                    {
                        "const": 1,
                        "description": "A member of the set."
                    }
                ]
            }
        },
        "arguments": [
            {
                "name": "key",
                "type": "key",
                "key_spec_index": 0
            },
            {
                "name": "member",
                "type": "string",
                "multiple": true
            }
        ]
    }
}
