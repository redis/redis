                            "minItems": 2,
                            "maxItems": 2,
                            "items": [
                                {
                                    "description": "Stream id",
                                    "type": "string",
                                    "pattern": "[0-9]+-[0-9]+"
                                },
                                {
                                    "oneOf": [
                                        {
                                            "description": "Array of field-value pairs",
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        },
                                        {
                                            "type": "null"
                                        }
                                    ]
                                }
                            ]
                        }
                    }
                }
            ]
        }
    }
}
