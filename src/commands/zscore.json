{
    "ZSCORE": {
        "summary": "Returns the score of a member in a sorted set.",
        "complexity": "O(1)",
        "group": "sorted_set",
        "since": "1.2.0",
        "arity": 3,
        "function": "zscoreCommand",
        "command_flags": [
            "READONLY",
            "FAST"
        ],
        "acl_categories": [
            "SORTEDSET"
        ],
        "key_specs": [
            {
                "flags": [
                    "RO",
                    "ACCESS"
                ],
                "begin_search": {
                    "index": {
                        "pos": 1
                    }
                },
                "find_keys": {
                    "range": {
                        "lastkey": 0,
                        "step": 1,
                        "limit": 0
                    }
                }
            }
        ],
        "reply_schema": {
            "oneOf": [
                {
                    "type": "number",
                    "description": "The score of the member (a double precision floating point number). In RESP2, this is returned as string."
                },
                {
                    "type": "null",
                    "description": "Member does not exist in the sorted set, or key does not exist."
                }
            ]
        },
        "arguments": [
            {
                "name": "key",
                "type": "key",
                "key_spec_index": 0
            },
            {
                "name": "member",
                "type": "string"
            }
        ]
    }
}
