{
    "SET": {
        "summary": "Set the string value of a key",
        "complexity": "O(1)",
        "group": "string",
        "since": "1.0.0",
        "arity": -3,
        "function": "setCommand",
        "history": [
            [
                "2.6.12",
                "Added the `EX`, `PX`, `NX` and `XX` options."
            ],
            [
                "6.0",
                "Added the `KEEPTTL` option."
            ],
            [
                "6.2",
                "Added the `!GET`, `EXAT` and `PXAT` option."
            ],
            [
                "7.0",
                "Allowed the `NX` and `!GET` options to be used together."
            ]
        ],
        "command_flags": [
            "write",
            "denyoom"
        ],
        "acl_categories": [
            "write",
            "string",
            "slow"
        ],
        "key_specs": [
            {
                "flags": [
                    "write",
                    "read"
                ],
                "begin_search": {
                    "index": {
                        "pos": 1
                    }
                },
                "find_keys": {
                    "range": {
                        "lastkey": 0,
                        "step": 1,
                        "limit": 0
                    }
                }
            }
        ],
        "returns":[
            {
                "description": "Command succeeded",
                "type":"simple-string",
                "constant_value": "+OK"
            },
            {
                "description": "Old value if `GET` was given",
                "type":"bulk-string"
            },
            {
                "description": "Either `SET` failed (with `NX` or `XX`), or `GET` was given and key didn't exist",
                "type": {
                    "RESP2":"null-bulk-string",
                    "RESP3":"null"
                }
            }
        ],
        "arguments": [
            {
                "name": "key",
                "type": "key",
                "value": "key"
            },
            {
                "name": "value",
                "type": "string",
                "value": "value"
            },
            {
                "name": "expiration",
                "type": "oneof",
                "value": [
                    {
                        "name": "ex",
                        "type": "integer",
                        "value": "seconds",
                        "token": "EX"
                    },
                    {
                        "name": "px",
                        "type": "integer",
                        "value": "milliseconds",
                        "token": "PX"
                    },
                    {
                        "name": "exat",
                        "type": "unix-time",
                        "value": "timestamp",
                        "token": "EXAT"
                    },
                    {
                        "name": "pxat",
                        "type": "unix-time",
                        "value": "milliseconds-timestamp",
                        "token": "PXAT"
                    },
                    {
                        "name": "keepttl",
                        "type": "pure-token",
                        "token": "KEEPTTL"
                    }
                ],
                "optional": true
            },
            {
                "name": "condition",
                "type": "oneof",
                "value": [
                    {
                        "name": "nx",
                        "type": "pure-token",
                        "token": "NX"
                    },
                    {
                        "name": "xx",
                        "type": "pure-token",
                        "token": "XX"
                    }
                ],
                "optional": true
            },
            {
                "name": "get",
                "token": "GET",
                "type": "pure-token",
                "optional": true
            }
        ]
    }
}
