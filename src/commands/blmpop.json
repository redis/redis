{
    "BLMPOP": {
        "summary": "Pop elements from a list, or block until one is available",
        "complexity": "O(N+M) where N is the number of provided keys and M is the number of elements returned.",
        "group": "list",
        "since": "7.0.0",
        "arity": -5,
        "function": "blmpopCommand",
        "get_keys_function": "blmpopGetKeys",
        "command_flags": [
            "WRITE",
            "BLOCKING"
        ],
        "acl_categories": [
            "LIST"
        ],
        "key_specs": [
            {
                "flags": [
                    "RW",
                    "ACCESS",
                    "DELETE"
                ],
                "begin_search": {
                    "index": {
                        "pos": 2
                    }
                },
                "find_keys": {
                    "keynum": {
                        "keynumidx": 0,
                        "firstkey": 1,
                        "step": 1
                    }
                }
            }
        ],
        "reply_schema": {
            "oneOf": [
                {
                    "description": "Operation timed-out",
                    "type": "null"
                },
                {
                    "description": "The key from which elements were popped and the popped elements",
                    "type": "array",
                    "minItems": 2,
                    "maxItems": 2,
                    "items": [
                        {
                            "description": "List key from which elements were popped.",
                            "type": "string"
                        },
                        {
                            "description": "Array of popped elements.",
                            "type": "array",
                            "minItems": 1,
                            "items": {
                                "type": "string"
                            }
                        }
                    ]
                }
            ]
        },
        "arguments": [
            {
                "name": "timeout",
                "type": "double"
            },
            {
                "name": "numkeys",
                "type": "integer"
            },
            {
                "name": "key",
                "type": "key",
                "key_spec_index": 0,
                "multiple": true
            },
            {
                "name": "where",
                "type": "oneof",
                "arguments": [
                    {
                        "name": "left",
                        "type": "pure-token",
                        "token": "LEFT"
                    },
                    {
                        "name": "right",
                        "type": "pure-token",
                        "token": "RIGHT"
                    }
                ]
            },
            {
                "token": "COUNT",
                "name": "count",
                "type": "integer",
                "optional": true
            }
        ]
    }
}
