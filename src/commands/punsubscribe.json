{
    "PUNSUBSCRIBE": {
        "summary": "Stops listening to messages published to channels that match one or more patterns.",
        "complexity": "O(N) where N is the number of patterns to unsubscribe.",
        "group": "pubsub",
        "since": "2.0.0",
        "arity": -1,
        "function": "punsubscribeCommand",
        "command_flags": [
            "PUBSUB",
            "NOSCRIPT",
            "LOADING",
            "STALE",
            "SENTINEL"
        ],
        "arguments": [
            {
                "name": "pattern",
                "type": "pattern",
                "optional": true,
                "multiple": true
            }
        ]
    }
}
