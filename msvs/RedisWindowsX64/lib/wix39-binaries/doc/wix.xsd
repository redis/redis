          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="Property" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation>Property tied to this group</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
