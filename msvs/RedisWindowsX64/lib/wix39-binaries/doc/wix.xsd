            </xs:documentation>
      <xs:appinfo>
        <xse:msiRef table="Billboard" href="http://msdn.microsoft.com/library/aa367823.aspx" />
        <xse:msiRef table="BBControl" href="http://msdn.microsoft.com/library/aa367818.aspx" />
      </xs:appinfo>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Billboard" maxOccurs="unbounded">
