        <xs:element ref="Dialog">
          <xs:annotation>
            <xs:documentation>Dialog specification, called from Sequence</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="DialogRef">
          <xs:annotation>
            <xs:documentation>Reference to a Dialog specification.</xs:documentation>
          </xs:annotation>
